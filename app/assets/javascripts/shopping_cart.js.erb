var shoppingCart = {};
function putItemInCart(id)
  {
    var text = $('#'+id).css("text-decoration");
    if (text == "line-through") {
      $('#'+id).css("text-decoration", "none");
      shoppingCart.push(id);
    }
    else {
      $('#'+id).css("text-decoration", "line-through");
      shoppingCart.push(id);
    }
  };
  
